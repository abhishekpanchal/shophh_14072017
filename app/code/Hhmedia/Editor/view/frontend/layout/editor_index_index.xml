<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Hhmedia\Editor\Block\Editor" name="editor.list" template="editor.phtml">
                <block class="Magento\Theme\Block\Html\Pager" name="editor_list_pager" as="editor_list_pager"/>
            </block>
        </referenceContainer>
        <referenceBlock name="page.main.title">
      		<action method="setPageTitle">
        		<argument translate="true" name="title" xsi:type="string">Featured Guest Editor</argument>
      		</action>
    	</referenceBlock>
    </body>
</page>